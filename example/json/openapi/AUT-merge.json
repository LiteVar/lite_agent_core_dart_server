{
  "openapi": "3.0.0",
  "info": {
    "title": "Zhuhai AUT Equipment API",
    "version": "1.0.0",
    "description": "AUT,Acoustic test,Audio test,Acoustic performance test,Audio performance test,station 3,module test,speaker test,microphone test,声学测试,声音品质测试,声学性能测试,音频测试,第3个测试,第3工站,喇叭测试,麦克风测试"
  },
  "servers": [
    {
      "url": "http://192.168.2.201:8090/v1/zhuhai"
    }
  ],
  "tags": [
    {
      "name": "AUT",
      "description": "AUT设备"
    }
  ],
  "paths": {
    "/AUT": {
      "post": {
        "tags": [
          "AUT"
        ],
        "summary": "AUT设备开始测试；查询设备状态，结果或数据",
        "description": "AUT(Acoustic test,Audio test,Acoustic performance test,Audio performance test,station 3,module test,speaker test,microphone test,声学测试,声音品质测试,声学性能测试,音频测试,第3个测试,第3工站,喇叭测试,麦克风测试)",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "operation": {
                    "type": "string",
                    "enum": [
                      "StartTest",
                      "status",
                      "result",
                      "data"
                    ],
                    "description": "操作类型，可选择StartTest(开始测试)|status(查询状态)|result(查询结果)|data(获取数据)",
                    "example": {
                      "operation": "StartTest"
                    }
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "操作执行成功",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "string",
                      "enum": [
                        "Idle",
                        "Testing"
                      ],
                      "description": "status(设备状态)"
                    },
                    "code": {
                      "type": "integer",
                      "description": "业务状态码"
                    },
                    "message": {
                      "type": "string",
                      "description": "描述信息"
                    },
                    "data": {
                      "type": "string",
                      "description": "详细数据"
                    }
                  }
                },
                "examples": {
                  "successResponse": {
                    "value": {
                      "code": 200,
                      "message": "成功开启测试"
                    }
                  },
                  "statusResponse": {
                    "value": {
                      "status": "Testing",
                      "code": 200
                    }
                  },
                  "resultResponse": {
                    "value": {
                      "code": 200,
                      "message": "PASS"
                    }
                  },
                  "dataResponse": {
                    "value": {
                      "code": 200,
                      "“message”": "PASS",
                      "data": []
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}